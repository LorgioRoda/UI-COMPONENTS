{
  "name": "ui-components-lr",
  "version": "0.3.0",
  "description": "Library for fun",
  "main": "./dist/index.js",
  "scripts": {
    "release": "npm run build && npm publish",
    "release:patch": "npm version patch && npm release",
    "release:minor": "npm version minor && npm publish",
    "release:major": "npm version major && npm publish",
    "build": "NODE_ENV=production && rm -rf ./dist && mkdir dist && npx babel --config-file ./babel.config.js src/ --out-dir dist --copy-files --ignore '**/*.spec.js' --no-copy-ignored",
    "test": "jest --config ./config/jest.config.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "library-ui",
    "react"
  ],
  "author": "Lorgio Roda",
  "license": "ISC",
  "dependencies": {
    "@emotion/css": "^11.10.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.7",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@jest/globals": "^29.5.0",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "babel-loader": "^8.2.3",
    "babel-preset-minify": "^0.5.1",
    "jest": "^27.5.1",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
